{
    "name": "Traffic Roads, railways and aerialways",
    "description": "Highways, Streets, Railways, Aerialways, etc.",
    "toggleable": true,
    "matches": {
        "geometry": "line",
        "anyTags": {
            "aerialway": {
                "*": true,
                "no": false
            },
            "waterway": {
                "*": true,
                "no": false
            },
            "railway": {
                "*": true,
                "no": false
            },
            "highway": {
                "motorway": true,
                "motorway_link": true,
                "trunk": true,
                "trunk_link": true,
                "primary": true,
                "primary_link": true,
                "secondary": true,
                "secondary_link": true,
                "tertiary": true,
                "tertiary_link": true,
                "residential": true,
                "unclassified": true,
                "living_street": true,
                "road": true
            }
        }
    }
}
