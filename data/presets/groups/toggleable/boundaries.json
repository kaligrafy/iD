{
    "name": "Boundaries",
    "description": "Administrative Boundaries",
    "toggleable": {
        "hiddenByDefault": true
    },
    "matches": {
        "geometry": ["point", "line", "area", "relation"],
        "anyTags": {
            "boundary": {
                "*": true,
                "no": false
            },
            "place": true
        },
        "allGroups": {
            "toggleable/traffic_roads": false,
            "toggleable/paths": false,
            "toggleable/landuse": false,
            "toggleable/building": false,
            "toggleable/barriers": false
        }
    }
}
